{
  "name": "react-loadable-ssr",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -r dist",
    "dev": "export NODE_ENV=development && yarn run build && yarn start",
    "build": "yarn run build:client && yarn run build:server",
    "start": "node dist/server/server.js",
    "build:client": "webpack --config=webpack.client.babel.js",
    "build:server": "webpack --config=webpack.server.babel.js",
    "analytics": "export NODE_ENV=production && webpack --config=webpack.analytics.babel.js"
  },
  "repository": "git@github.com:slashtu/react-loadable-ssr.git",
  "author": "Slash Tu <y12341234@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "isomorphic-unfetch": "^2.0.0",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "webpack": "3.11.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": "^8.9.0"
  }
}
